<h1>Login</h1>


<div class="row">
    <form id="loginForm" class="col s12">
      <div class="row">
        <div class="input-field col s12">
          <input id="email" placeholder="email" type="text" class="validate">
        </div>
      </div>
      <div class="row">
        <div class="input-field col s12">
          <input id="password" placeholder="password" type="password" class="validate">
          <label for="password"></label>
        </div>
      </div>
      <div class="row">
        <div class="input-field col s12">
          <a class="black btn" href="javascript:{}" onclick="document.getElementById('loginForm').submit()">Log In</a>
          <a class="black btn" href="/register/" onclick="document.getElementById('loginForm').submit()">Sign Up</a>
        </div>
      </div>
    </form>
  </div>