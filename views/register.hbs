<h1>Register</h1>

<div class="row">
    <form class="col s12">
      <div class="row">
        <div class="input-field col s6">
          <input id="first_name" placeholder="First Name" type="text" class="validate">
        </div>
        <div class="input-field col s6">
          <input id="last_name" placeholder="Last Name" type="text" class="validate">
        </div>
      </div>
      <div class="row">
        <div class="input-field col s12">
          <input id="email" placeholder="hitman@email.com" type="text" class="validate">
        </div>
      </div>
      <div class="row">
        <div class="input-field col s12">
          <input id="password" placeholder="password" type="password" class="validate">
        </div>
      </div>
      <div class="row">
        <div class="input-field col s12">
          <a class="black btn">Register</a>
        </div>
      </div>
    </form>
  </div>